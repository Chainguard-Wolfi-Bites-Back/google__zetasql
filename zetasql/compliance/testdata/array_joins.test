[name=array_right_join_to_null_array_on_clause]
SELECT a, b
FROM UNNEST(CAST(NULL AS ARRAY<INT64>)) a
RIGHT JOIN UNNEST([1, 1, 2, 3, 5, 8, 13, NULL]) b
  ON a = b
--
ARRAY<STRUCT<a INT64, b INT64>>[
  {NULL, 1},
